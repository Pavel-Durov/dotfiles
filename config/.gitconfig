# kimchi dotfiles
[user]
        name = Pavel Durov
        email = pav3ldurov@gmail.com
[pull]
        rebase = false
[filter "lfs"]
        smudge = git-lfs smudge -- %f
        process = git-lfs filter-process
        required = true
        clean = git-lfs clean -- %f
[core]
        autocrlf = input
[init]
        defaultBranch = main
[push]
        autoSetupRemote = true
